{
  "name": "essential-react",
  "version": "0.4.0",
  "description": "A minimal skeleton for building testable React apps using Babel",
  "main": "src/main.js",
  "scripts": {
    "postinstall": "npm run build",
    "server": "node server.js",
    "start": "webpack --config webpack.local.config.js",
    "build": "webpack -p --progress --config webpack.production.config.js",
    "test": "nyc ava --fail-fast --verbose --require babel-register",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pheuter/essential-react.git"
  },
  "keywords": [
    "skeleton",
    "template",
    "react",
    "quickstart"
  ],
  "author": "Mark Fayngersh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pheuter/essential-react/issues"
  },
  "homepage": "https://github.com/pheuter/essential-react",
  "devDependencies": {
    "autoprefixer": "^6.0.2",
    "ava": "^0.11.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "coveralls": "^2.11.2",
    "css-loader": "^0.18.0",
    "enzyme": "^1.4.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^2.7.2",
    "node-sass": "^3.4.2",
    "nyc": "^5.3.0",
    "postcss-loader": "^0.6.0",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "^0.14.6",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.2",
    "style-loader": "^0.12.3",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "ampersand-app": "^1.0.4",
    "ampersand-collection": "^1.6.1",
    "ampersand-model": "^6.0.2",
    "ampersand-react-connector": "^1.0.0",
    "ampersand-state": "^5.0.0",
    "async": "^1.5.2",
    "babel-core": "^6.4.5",
    "babel-polyfill": "^6.3.14",
    "babel-runtime": "^6.3.19",
    "can-use-dom": "^0.1.0",
    "classnames": "^2.2.3",
    "express": "^4.12.3",
    "flexboxgrid": "^6.3.0",
    "history": "^1.17.0",
    "lodash": "^4.1.0",
    "material-ui": "^0.14.4",
    "react": "^0.14.6",
    "react-addons-update": "^0.14.7",
    "react-dom": "^0.14.6",
    "react-flexbox-grid": "^0.9.4",
    "react-google-maps": "^4.7.2",
    "react-router": "^1.0.3",
    "react-tap-event-plugin": "^0.2.2"
  }
}
